using System;

class Program
{
    static void Main()
    {
        int maior = 0;
        string texto = "";
        string[] cars = {"Volvo", "BMW", "Ford", "Mazda"};
          foreach (string i in cars)
          {
            Console.WriteLine(i);
              if (i.Length > maior)
              {
                texto = i;
              }
          }
        Console.WriteLine("A maior string Ã©: " + texto);
    }
}
